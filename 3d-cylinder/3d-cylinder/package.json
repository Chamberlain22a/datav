                          },
                          "fontSize": {
                            "name": "字号",
                            "type": "number"
                          },
                          "align": {
                            "name": "对齐",
                            "type": "select",
                            "range": [
                              {
                                "name": "自动",
                                "value": "auto"
                              },
                              {
                                "name": "左对齐",
