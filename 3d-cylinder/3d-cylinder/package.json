                        "children": {
                          "width": {
                            "name": "折线宽度",
                            "type": "stepper",
                            "caption": "折线宽度",
                            "step": 1,
                            "suffix": "px"
                          },
                          "color": {
                            "name": "颜色",
                            "type": "color"
                          },
                          "type": {
                            "name": "类型",
                            "type": "select",
                            "options": [
