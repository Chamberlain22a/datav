                          },
                          "type": {
                            "name": "类型",
                            "type": "text"
                          },
                          "opacity": {
                            "name": "透明度",
                            "type": "number"
                          }
                        },
                        "fold": true
                      }
                    },
                    "fold": true
                  }
                }
              }
            }
          },
          "lineSeries": {
            "name": "柱图颜色系列",
            "mode": "single",
            "children": {
              "tabs": {
                "name": "颜色系列",
                "type": "tabs",
                "maxTabs": 10,
                "template": {
                  "name": "系列<%= i + 1%>",
                  "children": {
                    "areaStyle": {
                      "name": "面积样式",
                      "type": "color"
                    },
                    "areaStyle1": {
                      "name": "面积样式",
                      "type": "color"
                    },
                    "areaStyle2": {
                      "name": "面积样式",
