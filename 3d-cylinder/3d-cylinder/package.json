                      },
                      "lineStyle": {
                        "name": "轴线样式",
                        "type": "group",
                        "children": {
                          "color": {
                            "name": "颜色",
                            "type": "color"
                          },
                          "width": {
                            "name": "宽度",
                            "type": "number"
                          },
                          "type": {
                            "name": "类型",
                            "type": "text"
                          },
                          "opacity": {
                            "name": "透明度",
