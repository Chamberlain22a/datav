                    "fold": true
                  },
                  "axisLabel": {
                    "name": "轴标签",
                    "type": "group",
                    "children": {
                      "show": {
                        "name": "显示",
                        "type": "boolean",
                        "default": true
                      },
                      "interval": {
                        "name": "分割间隔",
                        "type": "number"
                      },
                      "inside": {
                        "name": "内部",
                        "type": "boolean"
                      },
                      "rotate": {
                        "name": "旋转",
                        "type": "number"
                      },
                      "margin": {
                        "name": "外间距",
