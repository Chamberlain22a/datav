{
  "name": "@futong.security/3D-cylinder",
  "version": "0.0.1",
  "dependencies": {
    "bcore": "0.0.18",
    "echarts": "5.3.0",
    "jquery": "2.1.4",
    "lodash": "4.6.1"
  },
  "datav": {
    "cn_name": "3d-自定义背景柱状图",
    "icon": "",
    "protocol": 2,
    "type": [
      "regular"
    ],
    "view": {
      "width": "400",
      "height": "200",
      "minWidth": "200",
      "minHeight": "100"
    },
    "apis": {
      "source": {
        "handler": "render",
        "description": "echarts柱状图",
        "fields": {
          "x": {
            "description": "x坐标轴值"
          },
          "y": {
            "description": "y坐标轴值"
          },
          "s": {
            "description": "系列值",
            "optional": true
          }
        }
      }
    },
    "config": {
      "options": {
        "name": "菜单",
        "type": "menu",
        "children": {
          "chart": {
            "name": "图表",
            "mode": "single",
            "children": {
              "margin": {
                "name": "自定义边距",
                "type": "group",
                "enableHide": true,
                "children": {
                  "top": {
                    "name": "上",
                    "type": "number"
                  },
                  "right": {
                    "name": "右",
                    "type": "number"
                  },
                  "bottom": {
                    "name": "下",
                    "type": "number"
                  },
                  "left": {
                    "name": "左",
                    "type": "number"
                  }
                }
              },
              "area": {
                "name": "面积样式",
                "type": "group",
                "children": {
                  "smooth": {
                    "name": "开启平滑曲线",
                    "type": "switch",
                    "caption": "开启平滑曲线"
                  },
                  "fillOpacity": {
                    "name": "透明度",
                    "type": "slider",
                    "step": 0.001,
                    "min": 0,
                    "max": 1,
                    "caption": "填充透明度"
                  }
                }
              },
              "line": {
                "name": "折线样式",
                "type": "group",
                "enableHide": true,
                "children": {
                  "width": {
                    "name": "折线宽度",
                    "type": "stepper",
                    "caption": "折线宽度",
                    "step": 1,
                    "suffix": "px"
                  }
                }
              },
              "point": {
                "name": "数据点标记",
                "type": "group",
                "enableHide": true,
                "children": {
                  "itemType": {
                    "name": "图例形状",
                    "type": "select",
                    "useFont": false,
                    "col": 12,
                    "options": [
                      {
                        "value": "emptyCircle",
                        "label": "emptyCircle"
                      },
                      {
                        "value": "circle",
                        "label": "circle"
                      },
                      {
                        "value": "rect",
                        "label": "rect"
                      },
                      {
                        "value": "roundRect",
                        "label": "roundRect"
                      },
                      {
                        "value": "triangle",
                        "label": "triangle"
                      },
                      {
                        "value": "pin",
                        "label": "pin"
                      },
                      {
                        "value": "arrow",
                        "label": "arrow"
                      },
                      {
                        "value": "none",
                        "label": "none"
                      }
                    ]
                  },
                  "itemSize": {
                    "type": "stepper",
                    "caption": "图例大小",
                    "min": 0,
                    "max": 10,
                    "step": 1,
                    "col": 12,
                    "suffix": "px"
                  }
                }
              },
              "legend": {
                "name": "图例",
                "type": "group",
                "children": {
                  "show": {
                    "name": "显示",
                    "type": "boolean",
                    "default": true
                  },
                  "orient": {
                    "name": "朝向",
                    "type": "select",
                    "range": [
                      {
                        "name": "水平",
                        "value": "horizontal"
                      },
                      {
                        "name": "垂直",
                        "value": "vertical"
                      }
                    ]
                  },
                  "align": {
                    "name": "对齐",
                    "type": "select",
                    "range": [
                      {
                        "name": "自动",
                        "value": "auto"
                      },
                      {
                        "name": "左对齐",
                        "value": "left"
                      },
                      {
                        "name": "居中对齐",
                        "value": "center"
                      },
                      {
                        "name": "右对齐",
                        "value": "right"
                      }
                    ]
                  },
                  "itemGap": {
                    "name": "元素间距",
                    "type": "number"
                  },
                  "textStyle": {
                    "name": "文本样式",
                    "type": "font",
                    "enableHide": false
                  }
                },
                "fold": true
              }
            }
          },
          "axis": {
            "name": "坐标轴",
            "mode": "multiple",
            "children": {
              "xAxis": {
                "name": "x轴",
                "type": "group",
                "children": {
                  "show": {
                    "name": "显示",
                    "type": "boolean",
                    "default": true
                  },
                  "boundaryGap": {
                    "name": "留白",
                    "type": "boolean"
                  },
                  "splitNumber": {
                    "name": "分割段数",
                    "type": "number"
                  },
                  "minInterval": {
                    "name": "最小间隔大小",
                    "type": "number"
                  },
                  "axisLabel": {
                    "name": "轴标签",
                    "type": "group",
                    "children": {
                      "show": {
                        "name": "显示",
                        "type": "boolean",
                        "default": true
                      },
                      "textStyle": {
                        "name": "文本样式",
                        "type": "font",
                        "enableHide": false
                      },
                      "customTickCount": {
                        "name": "自定义数量",
                        "type": "stepper",
                        "caption": "数量",
                        "step": 1
                      },
                      "interval": {
                        "name": "显示间隔",
                        "type": "stepper",
                        "caption": "数量",
                        "step": 1
                      },
                      "ellipsis": {
                        "name": "文本省略",
                        "type": "stepper",
                        "caption": "数量",
                        "step": 1
                      }
                    }
                  },
                  "axisTick": {
                    "name": "刻度标签",
                    "type": "group",
                    "children": {
                      "show": {
                        "name": "显示",
                        "type": "boolean",
                        "default": false
                      }
                    }
                  },
                  "axisLine": {
                    "name": "轴线",
                    "type": "group",
                    "children": {
                      "show": {
                        "name": "显示",
                        "type": "boolean",
                        "default": true
                      },
                      "lineStyle": {
                        "name": "轴线样式",
                        "type": "group",
                        "children": {
                          "color": {
                            "name": "颜色",
                            "type": "multicolor"
                          },
                          "width": {
                            "name": "宽度",
                            "type": "number"
                          },
                          "type": {
                            "name": "类型",
                            "type": "text"
                          },
                          "opacity": {
                            "name": "透明度",
                            "type": "number"
                          }
                        },
                        "fold": true
                      }
                    },
                    "fold": true
                  },
                  "splitLine": {
                    "name": "分割线",
                    "type": "group",
                    "children": {
                      "show": {
                        "name": "显示",
                        "type": "boolean",
                        "default": false
                      },
                      "interval": {
                        "name": "分割间隔",
                        "type": "number"
                      },
                      "lineStyle": {
                        "name": "轴线样式",
                        "type": "group",
                        "children": {
                          "width": {
                            "name": "宽度",
                            "type": "number"
                          },
                          "type": {
                            "name": "类型",
                            "type": "text"
                          },
                          "opacity": {
                            "name": "透明度",
                            "type": "number"
                          }
                        },
                        "fold": true
                      }
                    },
                    "fold": true
                  }
                }
              },
              "yAxis": {
                "name": "y轴",
                "type": "group",
                "children": {
                  "show": {
                    "name": "显示",
                    "type": "boolean",
